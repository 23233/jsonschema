{
	"$schema": "http://json-schema.org/draft/2020-12/schema",
	"$ref": "#/$defs/CustomSliceOuter",
	"$defs": {
	  "CustomSliceOuter": {
		"properties": {
		  "slice": {
			"$schema": "http://json-schema.org/draft/2020-12/schema",
			"$ref": "#/$defs/CustomSliceType"
		  }
		},
		"additionalProperties": false,
		"type": "object",
		"required": [
		  "slice"
		]
	  },
	  "CustomSliceType": {
		"oneOf": [
		  {
			"type": "string"
		  },
		  {
			"items": {
			  "type": "string"
			},
			"type": "array"
		  }
		]
	  }
	}
  }